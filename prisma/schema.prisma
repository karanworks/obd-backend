// Looking for ways to speed up your queries, or scale easily with your serverless or edge functions?
// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  username String
  email    String @unique
  password String
  token    Int      @default(0)
  roleId Int
  adminId Int?
  isActive Int      @default(0)
  userIp   String
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Menu{
  id Int @id @default(autoincrement())
  menuLableId String
  label String
  icon String
  link String
  status Int
  sequence Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
 
}

model SubMenu {
  id Int @id @default(autoincrement())
  submenuLableId String
  label String
  parentId String
  link String
  status Int
  menuId Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  
}

model SubMenuAssign{
  id Int @id @default(autoincrement())
  menuId Int
  status Int
  subMenuId Int
  roleId Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Role {
  id Int @id @default(autoincrement())
  name String
  status Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model RoleAssign{
  id Int @id @default(autoincrement())
  roleId Int
  userId Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Team {
  id Int @id @default(autoincrement())
  teamName String
  email String
  password String
  userType Int
  status Int?
  addedBy Int? 
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model Employee{
  id Int @id @default(autoincrement())
  employeeName String
  email String
  password String
  userType Int
  teamId Int
  status Int @default(1)
  addedBy Int? 
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}


model Target{
  id Int @id @default(autoincrement())
  teamId Int
  month String
  year Int
  target Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model CreditCardForm{
  id Int @id @default(autoincrement())
  fullName String
  mobileNo String
  email String
  dob String
  panNo String
  fatherName String
  motherName String
  employeeName String
  currentAddress String
  pinCode Int
  companyName String
  companyAddress String
  income Int
  bankName String
  bankId Int
  clientType String
  addedBy Int
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  


  @@index([employeeName])
  @@index([bankName])
}

model FormStatus{
  id Int @id @default(autoincrement())
  formId Int
  formStatus String
  bankStatus String?
  applicationNo String
  formType String
  bankId Int
  addedBy Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}


model LoanForm{
  id Int @id @default(autoincrement())
  employeeType String
  loanType String
  fullName String
  mobileNo String
  currentAddress String
  income Int
  pinCode Int
  addedBy Int
  panNo String
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model InsuranceForm{
  id Int @id @default(autoincrement())
  employeeType String
  insuranceType String
  fullName String
  mobileNo String
  currentAddress String
  income Int
  pinCode Int
  addedBy Int
  panNo String
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}
model DematAccountForm{
  id Int @id @default(autoincrement())
  employeeType String
  fullName String
  mobileNo String
  currentAddress String
  income Int
  pinCode Int
  addedBy Int
  panNo String
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model FormPermission {
  id Int @id @default(autoincrement())
  centerId Int
  formId Int
  addedBy Int
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model RawFormData{
  id Int @id @default(autoincrement())
  name String
  email String
  email2 String?
  gender String?
  address String?
  state String?
  stateId Int? 
  city String?
  cityId Int?
  country String?
  pinCode String?
  salary String
  salaryInNumbers Int?
  company String
  departmentPosition String
  dataType String
  mobile1 String @unique
  mobile2 String? @unique
  mobile3 String? @unique
  landline String?
  vendor String
  purchaseDate DateTime
  addedBy Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  

  @@index(mobile1)
  @@index(stateId)
  @@index(cityId)
  @@index(city)
  @@index(salary)
  @@index(salaryInNumbers)

}


model States {
  id Int @id @default(autoincrement())
  name String
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model Cities {
  id Int @id @default(autoincrement())
  name String
  stateId Int
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model PinCode{
  id Int @id @default(autoincrement())
  name String
  pinCode String
  cityId Int
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model OBDData{
  id Int @id @default(autoincrement())
  number String @unique
  camp String?
  leadset String?
  callStatus String?
  callStartTime String?
  callAnsdTime String?
  callEndTime String?
  ringTime String?
  talkTime String?
  callerId String?
  retryNo String?
  hangupReason String?
  dtmfKeyPress String?
  dtmfPressTime String?
  token String?
  fileName String?
  addedBy Int?
  loadDate String?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  

}

model SalaryInLacs{
  id Int @id @default(autoincrement())
  value String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model SalaryInThousands{
  id Int @id @default(autoincrement())
  value String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model BankStatus{
  id Int @id @default(autoincrement())
  applicationNo String?
  formId Int?
  comment String
  bankStatus String
  bankId Int
  status Int @default(1)
  formType String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) 
}

model Dropdown{
  id Int @id @default(autoincrement())
  name String 
  type String
  sequence Int
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) 
  
}

model DailyDialerReport{
  id Int @id @default(autoincrement())
  campaign String
  agentId String
  agentName String
  sip Int
  phoneNumber String
  callType String
  startTime String
  endTime String
  callHandlingTime String
  talkTime String
  wrapupDuration String
  disposition String
  subDisposition String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) 
}