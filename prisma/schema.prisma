// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  username String
  email    String @unique
  password String
  token    Int      @default(0)
  roleId Int
  adminId Int?
  isActive Int      @default(0)
  userIp   String
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Menu{
  id Int @id @default(autoincrement())
  menuLableId String
  label String
  icon String
  link String
  status Int
  sequence Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
 
}

model SubMenu {
  id Int @id @default(autoincrement())
  submenuLableId String
  label String
  parentId String
  link String
  status Int
  menuId Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  
}

model SubMenuAssign{
  id Int @id @default(autoincrement())
  menuId Int
  status Int
  subMenuId Int
  roleId Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Role {
  id Int @id @default(autoincrement())
  name String
  status Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model RoleAssign{
  id Int @id @default(autoincrement())
  roleId Int
  userId Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Campaigns {
  id Int @id @default(autoincrement())
  campaignName String
  welcomeMessageText String?
  welcomeMessageAudio String?
  invalidMessageText String?
  invalidMessageAudio String?
  timeOutMessageText String?
  timeOutMessageAudio String?
  gatewayId String
  confFileName String? // LOCATION
  dialplanName String? // NAME ONLY
  status Int @default(1)
  addedBy Int? 
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model Design {
   id Int @id @default(autoincrement())
   key String
   messageText String?
   messageAudio String?
   mobileNumber String?
   campaignId Int
   status Int @default(1)
   addedBy Int?
   createdAt DateTime @default(now())
   updatedAt DateTime @default(now())  
}

model CampaignDataSetting{
   id Int @id @default(autoincrement())
   campaignId Int
   workDays String?
   date String
   timeStart String
   timeEnd String
   status Int @default(1)
   createdAt DateTime @default(now())
   updatedAt DateTime @default(now())  
}

model CampaignDialingData{
  id Int @id @default(autoincrement())
  campaignDataSettingId Int
  campaignId Int
  phoneNumber String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}

model CampaignDialingDataStatus{
  id Int @id @default(autoincrement())
  campaignDialingDataId Int
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}


model CallResponseCDR{
 id Int @id @default(autoincrement())
 eventName String
 privilege String
 channelId String
 channelState String
 channelStateDesc String
 calleridnum String
 calleridname String
 connectedlinenum String
 connectedlinename String
 language String
 accountcode String
 context String
 exten String
 priority String
 uniqueid String @unique
 linkedid String
 cause String
 causeTxt String?
 createdAt DateTime @default(now())
 updatedAt DateTime @default(now())  
}
model Gateway{
  id Int @id @default(autoincrement())
  gatewayIpAddress String
  channels Int
  userId String
  password String
  status Int @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())  
}